<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="anchor" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TitledPane fx:id="titlePane" animated="false" wrapText="true">
         <content>
            <VBox fx:id="box">
               <children>
                  <Label fx:id="warnungen_TriggerText" maxWidth="600.0" text="Label" wrapText="true" />
                  <Label fx:id="hintergrund" text="Hintergrund: " underline="true" />
                  <Label fx:id="warnungen_beschreibung" maxWidth="550.0" text="Label" wrapText="true" />
               </children>
            </VBox>
         </content>
      </TitledPane>
   </children>
</AnchorPane>
